{
  "editor.tabSize": 2,
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "files.associations": {
    "*.json": "jsonc"
  },
  "workbench.iconTheme": "vscode-great-icons",
  "editor.fontFamily": "JetBrains Mono,JetBrainsMono-Thin, Cascadia Code, Consolas, 'Courier New', monospace",
  "editor.fontWeight": "normal",
  "editor.inlineSuggest.enabled": true,
  "files.eol": "auto",
  "window.zoomLevel": 1,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "terminal.integrated.profiles.windows": {
    "PShell": {
      "path": "D:\\ProgramFiles\\Git\\bin\\bash.exe",
    }
  },
  "terminal.integrated.defaultProfile.windows": "PShell",
  // "terminal.integrated.shell.windows": "D:\\ProgramFiles\\Git\\bin\\bash.exe",
  "editor.formatOnSave": true,
  "cSpell.words": ["ahooks"]
}
